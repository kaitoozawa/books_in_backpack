<div>
  <h1>Recieved Request for your <%= current_user.book.title %></h1>
  <ul>
    <% @requested_users.each do |user| %>
      <li><%= link_to user.profile.name, profile_path(id: user.profile.id) %> who has <%= link_to user.book.title, book_path(user.book.id) %></li>
      <%= render 'relationships/request_button', user: user %>
    <% end %>
  </ul>
</div>
<div>
  <%= link_to 'Back to Search', searches_path %>
</div>